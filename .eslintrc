{
  "extends": [
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "prettier",
    "@typescript-eslint"
  ],
  "rules": {
    "no-multi-spaces": "off",
    "indent": "off",
    "comma-dangle": "off",
    "spaced-comment": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "trailingComma": "es5",
        "semi": false
      }
    ]
  }
}
